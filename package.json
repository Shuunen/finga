{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/finga/issues",
  "description": "Finger pointing, ish, cham cham cham - online game",
  "devDependencies": {
    "npm-run-all": "*",
    "shuunen-stack": "0.9.6",
    "shuutils": "*",
    "tailbuild": "1.1.1"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/Shuunen/finga",
  "keywords": [
    "finger pointing",
    "ish",
    "cham cham cham",
    "online",
    "game"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/finga",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/finga.git"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css -- --production",
    "build:js": "stack build src/index.ts --out-dir public --format iife --minify",
    "build:css": "tailbuild public/index.css --files public/*.{js,html}",
    "ci": "run-s build lint test",
    "dev": "run-p dev:*",
    "dev:css": "npm run build:css -- --watch",
    "dev:js": "npm run build:js -- --dev",
    "lint": "stack lint",
    "postversion": "git push && git push --tags",
    "preversion": "npm run ci",
    "start": "npm run dev",
    "test": "echo no tests yet",
    "update": "stack update"
  },
  "version": "0.0.1"
}
